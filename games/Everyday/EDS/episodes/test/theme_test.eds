DEF EPISODE "ThemeTest"
    SET CHOOSABLE FALSE

    ROUND <1 8:00> <1 22:00> 2000 1000 1

    SCHEDULE TASK "MiscThemeTest" <1 8:00>
    SCHEDULE TASK "CleaningThemeTest" <1 8:00>
    SCHEDULE TASK "FixingThemeTest" <1 8:00>
    SCHEDULE TASK "CookingThemeTest" <1 8:00>
